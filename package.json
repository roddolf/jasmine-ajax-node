{
  "name": "jasmine-ajax-node",
  "version": "1.1.1",
  "description": "Request mocking for Node.js using the same API as jasmine-ajax.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build",
    "build": "npm run clean && npm run compile",
    "compile": "rollup --config rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:sendCoverage": "codecov",
    "lint": "eslint . --ext .ts",
    "clean": "npm run clean:dist && npm run clean:temp",
    "clean:dist": "del-cli dist",
    "clean:temp": "del-cli temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roddolf/jasmine-ajax-node.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "jasmine",
    "jasmine-ajax",
    "http",
    "request",
    "mocking"
  ],
  "author": "Rodolfo Aguirre <aguirreg.rodolfo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roddolf/jasmine-ajax-node/issues"
  },
  "homepage": "https://github.com/roddolf/jasmine-ajax-node",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.36",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "codecov": "^3.8.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "gulp-filter": "^7.0.0",
    "jest": "^27.3.1",
    "rollup": "^2.58.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "jasmine": ">=2"
  }
}
